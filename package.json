{
	"name": "tidrane-backend",
	"version": "1.0.0",
	"description": "Tidrane backend",
	"main": "src/server.ts",
	"author": "Hachemi Hamadi",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"scripts": {
		"start": "tsc-alias && npm run build:prod && node dist/server.js",
		"dev": "nodemon",
		"build:prod": "swc src -d dist --source-maps --copy-files",
		"build:dev": "swc src -d dist --source-maps --copy-files",
		"check-types": "tsc --noemit",
		"lint": "eslint --ignore-path .gitignore --ext .ts src/ && npm run check-types",
		"lint:fix": "npm run lint --fix",
		"test": "APP_ENV=test tsx --test test/**/*.test.ts",
		"start:api": "node dist/server.js",
		"start:worker": "node dist/worker.js",
		"dev:concurrent": "concurrently \"npm:dev:api\" \"npm:dev:worker\"",
		"dev:api": "nodemon",
		"dev:worker": "tsx src/worker.ts",
		"cli": "tsx src/cli/index.ts",
		"migration:generate": "npm run cli migration generate",
		"migration:run": "npm run cli migration run",
		"migration:status": "npm run cli migration status",
		"migration:rollback": "npm run cli migration rollback",
		"migration:fresh": "npm run cli migration fresh",
		"seed:all": "npm run cli seed run"
	},
	"imports": {
		"#/*": "./dist/*",
		"#config": "./dist/config/index.js",
		"#interfaces/*": "./dist/interfaces/*",
		"#utils/*": "./dist/utils/*"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.842.0",
		"@aws-sdk/lib-storage": "^3.842.0",
		"@aws-sdk/s3-request-presigner": "^3.842.0",
		"@fastify/compress": "^8.1.0",
		"@fastify/cors": "^11.0.1",
		"@fastify/env": "^5.0.2",
		"@fastify/formbody": "^8.0.2",
		"@fastify/helmet": "^13.0.1",
		"@fastify/static": "^8.2.0",
		"@fastify/swagger": "^9.5.1",
		"@mikro-orm/cli": "^6.4.16",
		"@mikro-orm/core": "^6.4.16",
		"@mikro-orm/entity-generator": "^6.4.16",
		"@mikro-orm/migrations": "^6.4.16",
		"@mikro-orm/postgresql": "^6.4.16",
		"@mikro-orm/reflection": "^6.4.16",
		"ajv-errors": "^3.0.0",
		"awilix": "^12.0.5",
		"bullmq": "^5.12.13",
		"class-validator": "^0.14.2",
		"fastify": "^5.4.0",
		"fastify-plugin": "^5.0.1",
		"fastify-print-routes": "^4.1.0",
		"googleapis": "^157.0.0",
		"ioredis": "^5.6.1",
		"json-schema-to-ts": "^3.1.1",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"pino": "^9.7.0",
		"pino-pretty": "^13.0.0",
		"redlock": "^5.0.0-beta.2",
		"reflect-metadata": "^0.2.2",
		"scrypt-kdf": "^3.0.0",
		"serialize-error": "^12.0.0",
		"ulid": "^3.0.1",
		"unique-names-generator": "^4.7.1",
		"zod": "^3.25.74"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@mikro-orm/seeder": "^6.4.16",
		"@swc/cli": "^0.7.7",
		"@swc/core": "^1.12.9",
		"@swc/helpers": "^0.5.17",
		"@types/bcrypt": "5.0.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/lodash": "^4.17.0",
		"@types/luxon": "^3.4.2",
		"@types/node": "20.4.6",
		"@types/supertest": "^6.0.3",
		"concurrently": "^9.2.0",
		"lint-staged": "13.2.3",
		"mongodb-memory-server": "^10.1.4",
		"nodemon": "^3.1.10",
		"source-map-support": "0.5.21",
		"supertest": "^7.1.1",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.7",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	}
}
